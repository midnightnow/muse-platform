{
  "name": "muse-platform-frontend",
  "version": "1.0.0",
  "description": "React frontend for MUSE Platform - Computational Platonism Creative Discovery",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "test:e2e:ci": "start-server-and-test dev http://localhost:5173 test:e2e:headless"
  },
  "keywords": [
    "muse",
    "computational-platonism",
    "creative-discovery",
    "frequency-signatures",
    "sacred-geometry",
    "archetypal-mathematics"
  ],
  "author": "MUSE Platform Team",
  "license": "MIT",
